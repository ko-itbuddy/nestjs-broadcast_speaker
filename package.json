  "scripts": {
    "init": "bower install && npm install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rimraf dist && cross-env NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "rimraf dist && cross-env NODE_ENV=development nodemon --config config/nodemon-debug.json",
    "start:testserver": "cross-env NODE_ENV=testserver node dist/src/main.js",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main.js",
    "start:pm2": "cross-env NODE_ENV=production pm2 start dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=development jest --forceExit",
    "test:watch": "cross-env NODE_ENV=development jest --watch --forceExit",
    "test:cov": "cross-env NODE_ENV=development jest --coverage --forceExit",
    "test:debug": "cross-env NODE_ENV=development node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --forceExit",
    "test:e2e": "cross-env NODE_ENV=development jest --config ./test/jest-e2e.json --forceExit",
    "bro-sync": "browser-sync start --proxy \"localhost:3030\" --files \"resources\""
  },
